# WAN 2.2 Prompt Enhancer Configuration
# Copy this file to .env and fill in your values

# Google Gemini API Key (required)
# Get yours at: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your-api-key-here

# Gemini Model to use (optional)
# Available models: gemini-2.5-flash, gemini-1.5-pro, gemini-1.5-flash
# Default: gemini-2.5-flash (fastest, good quality)
GEMINI_MODEL=gemini-2.5-flash

# Optional: Set thinking budget (0 = fastest, higher = more deliberate)
# Default: 0 for fast responses
GEMINI_THINKING_BUDGET=0